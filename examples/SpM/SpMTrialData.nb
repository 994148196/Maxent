  RowBox[{"Gauss", "[", 
   RowBox[{"x_", ",", "\[Mu]_", ",", " ", "width_", ",", " ", "weight_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "y", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Sigma]", "=", 
      RowBox[{"width", "/", 
